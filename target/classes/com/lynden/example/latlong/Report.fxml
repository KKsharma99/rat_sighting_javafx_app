               <font>
                  <Font name="Yu Gothic UI Regular" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="stAddress" prefHeight="30.0" prefWidth="163.0" />
            <Label prefHeight="29.0" prefWidth="34.0" />
            <Label prefHeight="30.0" prefWidth="41.0" text="ZIP:" textFill="WHITE">
               <font>
                  <Font name="Yu Gothic UI Regular" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="zip_text" prefHeight="30.0" prefWidth="79.0" />
         </children>
      </HBox>
      <Button layoutX="245.0" layoutY="346.0" mnemonicParsing="false" onAction="#addSighting" prefHeight="32.0" prefWidth="111.0" style="-fx-border-radius: 0px; -fx-background-color: #979797; -fx-text-fill: white; -fx-border-color: grey; -fx-border-width: 0.3px;" text="Submit" />
      <HBox layoutX="92.0" layoutY="218.0" prefHeight="32.0" prefWidth="417.0">
         <children>
            <ChoiceBox fx:id="burough" prefHeight="30.0" prefWidth="167.0" />
            <Label prefHeight="29.0" prefWidth="36.0" />
            <ChoiceBox fx:id="locationType" prefHeight="30.0" prefWidth="214.0" />
         </children>
      </HBox>
      <HBox layoutX="92.0" layoutY="268.0" prefHeight="32.0" prefWidth="417.0">
         <children>
            <Label prefHeight="30.0" prefWidth="80.0" text="Latitude:" textFill="WHITE">
               <font>
                  <Font name="Yu Gothic UI Regular" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="latitude" prefHeight="30.0" prefWidth="105.0" />
            <Label prefHeight="29.0" prefWidth="39.0" />
            <Label prefHeight="30.0" prefWidth="93.0" text="Longitude:" textFill="WHITE">
               <font>
                  <Font name="Yu Gothic UI Regular" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="longitude" prefHeight="30.0" prefWidth="105.0" />
         </children>
      </HBox>
      <Label fx:id="error" layoutX="92.0" layoutY="103.0" textFill="RED">
         <font>
            <Font name="Yu Gothic UI Regular" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
